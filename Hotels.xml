<?xml version="1.0" encoding="utf-8"?>
<!-- Root element holding all hotel records -->
<Hotels xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://YourGitHubUsername.github.io/YourRepoName/Hotels.xsd">
  
  <!-- First hotel with a rating -->
  <Hotel Rating="4.2">
    <!-- Hotel name -->
    <Name>Westin</Name>
    <!-- Multiple phone numbers -->
    <Phone>480-968-8885</Phone>
    <Phone>800-937-8461</Phone>
    <!-- Hotel address -->
    <Address NearestAirport="Sky Harbor">
      <Number>11</Number>
      <Street>E.7th St</Street>
      <City>Tempe</City>
      <State>AZ</State>
      <Zip>85281</Zip>
    </Address>
  </Hotel>
  
  <!-- Second hotel without a rating -->
  <Hotel>
    <Name>Example Hotel Inn</Name>
    <Phone>123-456-7890</Phone>
    <Address NearestAirport="Example Airport">
      <Number>100</Number>
      <Street>Main St</Street>
      <City>Anytown</City>
      <State>CA</State>
      <Zip>12345</Zip>
    </Address>
  </Hotel>
  
</Hotels>
